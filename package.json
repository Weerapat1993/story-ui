    "storybook-readme": "^4.0.0-beta1",
    "style-loader": "^0.23.0",
    "webpack": "^4.18.0"
  },
  "jest": {
    "preset": "@immowelt/jest-preset-react",
    "setupTestFrameworkScriptFile": "<rootDir>/src/config/enzyme.js",
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/config/",
      "<rootDir>/node_modules/"
    ],
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react/jsx-filename-extension": 0,
      "import/prefer-default-export": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "import/no-unresolved": 0,
      "jsx-a11y/href-no-hash": 0
    },
    "settings": {
      "import/resolver": "webpack"
    }
  },
  "eslintIgnore": [
    "*.test.js",
    "*.spec.js"
  ]
}
