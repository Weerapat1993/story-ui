    "popmotion": "^8.3.8",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-loading-skeleton": "^0.6.1",
    "react-pose": "^3.3.3",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.82.5",
    "styled-components": "^3.4.6"
  },
  "devDependencies": {
    "@immowelt/jest-preset-react": "^1.1.3",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-backgrounds": "^3.4.10",
    "@storybook/addon-info": "^3.4.10",
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addon-notes": "^3.4.10",
    "@storybook/addon-options": "^3.4.10",
    "@storybook/addon-viewport": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.7.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.0",
    "node-sass": "^4.9.3",
    "react-element-to-jsx-string": "^14.0.1",
    "react-error-overlay": "^4.0.1",
    "react-test-renderer": "^16.5.1",
